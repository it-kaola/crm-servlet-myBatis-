<%@ page contentType="text/html;charset=UTF-8" language="java" %>

<%
	String basePath = request.getScheme() + "://" + request.getServerName() + ":" + request.getServerPort() + request.getContextPath() + "/";
	/*
	上面这条语句得到的结果是：basePath=http://127.0.0.1:8080/crm/
	*/
%>

<html>
<head>
	<base href="<%=basePath%>">
	<meta charset="UTF-8">

	<link href="jquery/bootstrap_3.3.0/css/bootstrap.min.css" type="text/css" rel="stylesheet" />
	<script type="text/javascript" src="jquery/jquery-1.11.1-min.js"></script>
	<script type="text/javascript" src="jquery/bootstrap_3.3.0/js/bootstrap.min.js"></script>

	<script type="text/javascript">

		//默认情况下取消和保存按钮是隐藏的
		var cancelAndSaveBtnDefault = true;

		$(function(){
			$("#remark").focus(function(){
				if(cancelAndSaveBtnDefault){
					//设置remarkDiv的高度为130px
					$("#remarkDiv").css("height","130px");
					//显示
					$("#cancelAndSaveBtn").show("2000");
					cancelAndSaveBtnDefault = false;
				}
			});

			$("#cancelBtn").click(function(){
				//显示
				$("#cancelAndSaveBtn").hide();
				//设置remarkDiv的高度为130px
				$("#remarkDiv").css("height","90px");
				cancelAndSaveBtnDefault = true;
			});

			$(".remarkDiv").mouseover(function(){
				$(this).children("div").children("div").show();
			});

			$(".remarkDiv").mouseout(function(){
				$(this).children("div").children("div").hide();
			});

			$(".myHref").mouseover(function(){
				$(this).children("span").css("color","red");
			});

			$(".myHref").mouseout(function(){
				$(this).children("span").css("color","#E6E6E6");
			});

			// 为关联市场活动的模态窗口中的全选复选框绑定事件
			$("#checkAllBox").click(function () {
				$("input[name=normalCheckBox]").prop("checked", this.checked);
			});

			// 为关联市场活动的模态窗口中动态生成的普通复选框绑定事件
			$("#actSearchTBody").on("click", $("input[name=normalCheckBox]"), function () {
				$("#checkAllBox").prop("checked", ($("input[name=normalCheckBox]").length) == ($("input[name=normalCheckBox]:checked").length));
			});


			// 为关联市场活动模态窗口中的搜索框绑定事件，通过按下回车键，查询并展现所需的市场活动列表
			$("#actName").keydown(function (event) {
				// 当敲下回车键后进行搜索
				if(event.keyCode == 13){

					// alert("展现市场活动列表");

					$.ajax({
						url : "workbench/clue/getActListByNameAndNotByClueId.do",
						data : {"actName" : $.trim($("#actName").val()), "clueId" : "${clue.id}"},
						type : "post",
						dataType : "json",
						success : function (respData) {
							// respData 应该为 [{市场活动1}, {市场活动2},....]

							var html = "";

							$.each(respData, function (index, element) {
								html += '<tr>';
								html += '<td><input type="checkbox" name="normalCheckBox" value="'+ element.id +'"/></td>';
								html += '<td>'+ element.name +'</td>';
								html += '<td>'+ element.startDate +'</td>';
								html += '<td>'+ element.endDate +'</td>';
								html += '<td>'+ element.owner +'</td>';
								html += '</tr>';
							});

							// 向表格中填充数据
							$("#actSearchTBody").html(html);
						}

					});

					// 加上return false，防止按下回车键后，模态窗口强制刷新，清空数据
					return false;  // 如果不加这行代码，当按下回车键后，模态窗口会默认强制刷新当前页面并清空作用域中的数据，页面不显示数据
				}

			});

			// 为关联按钮绑定事件，执行批量关联市场活动操作
			$("#bundBtn").click(function () {

				// 获取复选框被选中的元素
				var $normalCheckedBox = $("input[name=normalCheckBox]:checked");

				// 拼接字符串，最后向浏览器发送的请求应该为：/workbench/clue/bundClueAndAct.do?clueId=xxx&actId=xxx&actId=xxx.....
				var parm = "clueId=${clue.id}";

				// 数组长度不为0，表示用户一定选中了要关联的市场活动，关联才有意义，才需要发送ajax请求
				if ($normalCheckedBox.length != 0){
					parm += "&";
					$.each($normalCheckedBox, function (index, element) {
						parm += "actId=" + element.value;
						if(index < $normalCheckedBox.length-1){
							parm += "&";
						}
					});

					$.ajax({
						url : "workbench/clue/bundClueAndAct.do",
						data : parm,
						type : "post",
						dataType : "json",
						success : function (respData) {
							// respData 应该为 {"isSuccess" : true/false}
							if(respData.isSuccess){
								// 展示关联的市场活动列表
								showActivityList();
								// 取消选中的全选复选框，清空搜索栏上的内容(这个在打开关联市场活动的超链接时已经做了，这里就不重复写了)
								// 关闭关联市场活动的模态窗口
								$("#bundModal").modal("hide");
							}else{
								alert("关联市场活动失败");
							}
						}
					});
				}else{
					// 程序能够执行到此处，说明用户没有选择要关联的市场活动，但却点击了关联按钮，应该直接关闭模态窗口
					$("#bundModal").modal("hide");
				}
			});


			// 页面加载完毕后，显示线索关联的市场活动列表
			showActivityList();


		});

		// 展现关联的市场活动列表
		function showActivityList() {
			$.ajax({
				url : "workbench/clue/getActListByClueId.do",
				data : {"clueId" : "${clue.id}"},
				type : "get",
				dataType : "json",
				success : function (respData) {
					// respData 应该为 [{市场活动1}, {市场活动2}, .....]

					var html = "";
					$.each(respData, function (index, element) {
						html += '<tr>';
						html += '<td>'+ element.name +'</td>';
						html += '<td>'+ element.startDate +'</td>';
						html += '<td>'+ element.endDate +'</td>';
						html += '<td>'+ element.owner +'</td>'; // 注意这个owner的值】
						// 这里的element.id不是市场活动的id值，而是关系表中对应的id值，这样方便后面的接触关联关系的操作
						html += '<td><a href="javascript:void(0);" onclick="debundClueAndAct(\''+ element.id +'\')" style="text-decoration: none;"><span class="glyphicon glyphicon-remove"></span>解除关联</a></td>';
						html += '</tr>';
					});

					// 向表格中填充数据
					$("#activityTbody").html(html);
				}

			});
		}

		// 解除市场活动与线索的关联关系
		function debundClueAndAct(id) { // 这个id就是关联关系表中对应的id

			if(confirm("确定解除关系吗？")){
				$.ajax({
					url : "workbench/clue/debundClueAndAct.do",
					data : {"id" : id},
					type : "post",
					dataType : "json",
					success : function (respData) {
						// respData 应该为 {"isSuccess" : true/false}
						if(respData.isSuccess){
							// 刷新市场活动关联列表
							showActivityList();
						}else{
							alert("解除关系失败");
						}
					}
				})
			}
		}

		function openBundAct() {
			// 清空搜索框中的内容
			$("#actName").val("");
			// 清空搜索结果
			$("#actSearchTBody").html("");
			// 取消选中的全选复选框
			$("#checkAllBox").prop("checked", false);
			// 展示关联市场活动的模态窗口
			$("#bundModal").modal("show");
		}

	</script>

</head>
